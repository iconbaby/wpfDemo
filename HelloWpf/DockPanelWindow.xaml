<Window x:Class="HelloWpf.DockPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWpf"
        mc:Ignorable="d"
        Title="DockPanelWindow" Height="450" Width="800">
    <DockPanel Margin="10" LastChildFill="False">
        <Button DockPanel.Dock="Top" Height="50">bt2</Button>
        <Button DockPanel.Dock="Bottom" Height="50">bt4</Button>
        <Button DockPanel.Dock="Left" Width="50">bt1</Button>
        <Button DockPanel.Dock="Right">bt3</Button>
        <Button DockPanel.Dock="Left">bt5</Button>
        
        
    </DockPanel>
</Window>
